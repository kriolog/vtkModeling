<ServerManagerConfiguration>
  <ProxyGroup name="filters">
 <SourceProxy name="MovingLeastSquare" class="vtkMovingLeastSquare"
     label="Moving Least Square">
     <Documentation
        long_help="Moving Least Square (currently only works for 2D)."
        short_help="Moving Least Square (currently only works for 2D).">
        See: Scott Schaefer, "Image Deformation Using Moving Least Square"
     </Documentation>
     <InputProperty
        name="Input"
        command="SetInputConnection">
           <ProxyGroupDomain name="groups">
             <Group name="sources"/>
             <Group name="filters"/>
           </ProxyGroupDomain>
           <DataTypeDomain name="input_type">
             <DataType value="vtkPolyData"/>
           </DataTypeDomain>
      </InputProperty>

      <InputProperty
        name="ControlPoints"
        label="Control Points"
        port_index="1"
        command="SetInputConnection">
        <Documentation>
          Deformed Control Points
        </Documentation>
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPointSet"/>
        </DataTypeDomain>
        <InputArrayDomain
          name="control_point_ids"
          attribute_type="point"/>
      </InputProperty>
      <StringVectorProperty
        command="SetInputArrayToProcess"
        element_types="0 0 0 0 2"
        name="ControlPointsIds"
        label="Control Point IDs"
        number_of_elements="5">
        <ArrayListDomain
          attribute_type="Scalars"
          data_type="VTK_ID_TYPE"
          number_of_elements="1"
          name="array_list">
          <RequiredProperties
            input_domain_name="control_point_ids">
            <Property
              name="ControlPoints"
              function="Input" />
          </RequiredProperties>
        </ArrayListDomain>
        <Documentation>
          Indices of control points
        </Documentation>
      </StringVectorProperty>

      <IntVectorProperty command="SetIsAffine"
                         name="IsAffine"
                         default_values="0"
                         number_of_elements="1">
      <BooleanDomain name="bool" />
      <Documentation>
        Using Affine transformation.
      </Documentation>
      </IntVectorProperty>

      <IntVectorProperty command="SetIsSimilarity"
                         name="IsSimilarity"
                         default_values="0"
                         number_of_elements="1">
      <BooleanDomain name="bool" />
      <Documentation>
        Using Similarity transformation.
      </Documentation>
      </IntVectorProperty>

      <IntVectorProperty command="SetIsRigid"
                         name="IsRigid"
                         default_values="0"
                         number_of_elements="1">
      <BooleanDomain name="bool" />
      <Documentation>
        Using Rigid transformation.
      </Documentation>
      </IntVectorProperty>

      <IntVectorProperty command="SetDimension"
                         name="Dimension"
                         default_values="2"
                         number_of_elements="1">
      <IntRangeDomain min="2"
                      max="3"
                      name="range" />
      <Documentation>
        2D or 3D, default value is 2 (3 doesn't work)
      </Documentation>
      </IntVectorProperty>

      <DoubleVectorProperty command="SetAlpha"
                         name="Alpha"
                         default_values="1.5"
                         number_of_elements="1">
      <Documentation>
        Alpha value for weight. larger then 1 is a better choice.
      </Documentation>
      </DoubleVectorProperty>

    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>